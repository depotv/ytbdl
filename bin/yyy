#!/bin/bash

function dl() {
  yid=${1:-}
  fn=${2:-}
  fp=$(endir)/'%(title)s.%(ext)s'
  # fp=$(endir)/$fn.mp3
  youtube-dl "$yid" -i -o "$fp" -x \
    --audio-format mp3 --audio-quality 320k
}

function endir() {
  dir=$(date +%y)q$(( ($(date +%-m)-1)/3+1 ))
  echo $dir
}

mkdir -p endir

yid=${1:-}
f=${2:-}

dl $yid "${f}"

